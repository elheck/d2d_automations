# Copy this file to backup.env and fill in your values
# backup.env is gitignored and stays on the server only

DB_PATH="/var/lib/docker/volumes/<volume_name>/_data/inventory.db"
RCLONE_CONFIG="/home/<user>/.config/rclone/rclone.conf"
R2_BUCKET="r2:<bucket-name>"
BACKUP_DIR="/tmp/inventory_backups"
KEEP_DAYS=7
